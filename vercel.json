{
  "redirects": [
    {
      "source": "/(.*)",
      "destination": "https://www.stokpro.shop/$1",
      "permanent": true,
      "has": [
        {
          "type": "host",
          "value": "stokpro.vercel.app"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}